{
    "name": "Time Reminder", 
    "description": "Don't forget to check in your time card!",
    "version": "0.0.1",
    "manifest_version": 2,
    "app": {
        "background": {
            "scripts": ["js/background.js"]
        }
    },
    "icons": {
        "16": "resources/icon.png", "128" : "resources/icon.png"
    },
    "permissions" : [
        "notifications",
        "storage"
    ]
}
